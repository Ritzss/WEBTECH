{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "marquee": "off"
      }
    ],
    "no-inline-styles": "off",
    "meta-viewport": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "input[type=month]",
          "input[type=week]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "input-image-alt": "off",
        "image-alt": "off"
      }
    ],
    "axe/forms": "off"
  }
}